<p id="notice"><%= notice %></p>

<h1>Estudios</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Description</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estudios.each do |estudio| %>
      <tr>
        <td><%= estudio.code %></td>
        <td><%= estudio.name %></td>
        <td><%= estudio.description %></td>
        <td><%= estudio.category %></td>
        <td><%= link_to 'Show', estudio %></td>
        <td><%= link_to 'Edit', edit_estudio_path(estudio) %></td>
        <td><%= link_to 'Destroy', estudio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<br/>

