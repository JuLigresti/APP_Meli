<p id="notice"><%= notice %></p>

<h1>Estudio Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Code</th>
      <th>Instructions</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estudio_types.each do |estudio_type| %>
      <tr>
        <td><%= estudio_type.name %></td>
        <td><%= estudio_type.code %></td>
        <td><%= estudio_type.instructions %></td>
        <td><%= link_to 'Show', estudio_type %></td>
        <td><%= link_to 'Edit', edit_estudio_type_path(estudio_type) %></td>
        <td><%= link_to 'Destroy', estudio_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Estudio Type', new_estudio_type_path %>
